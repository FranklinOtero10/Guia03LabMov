<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.guia03" >

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Guia03"
        tools:targetApi="31" >
        <activity
            android:name=".AcercaDeActivity"
            android:exported="false"
            android:label="@string/name_acerca"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".MostrarListaActivity"
            android:exported="false"
            android:label="@string/name_ListarActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".TrabajadorTiempoCompletoActivity"
            android:exported="false"
            android:label="@string/name_trabajadorTTCActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".TrabajadorHoraActivity"
            android:exported="false"
            android:label="@string/name_trabajadorHoraActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".SeleccionarTrabajadorActivity"
            android:exported="false"
            android:label="@string/name_activity_select"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>